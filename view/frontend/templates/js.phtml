<script>

    console.log(kjua);
    console.log(jQuery);

    require(
        ['jquery'],
        function($) {

            var status = false,
                stTimer,
                timerId,
                custom;

            $(document).on('change', '.mercury_coin_val', function (event) {
                event.preventDefault();

                let selectedCoin = $('.mercury_coin_val').val();

                $.ajax({
                    type: 'POST',
                    url: '<?php echo $this->getAjaxUrl() ?>',
                    data: {'crypto': selectedCoin},
                    dataType: 'json',
                    beforeSend: function (xhr) {
                        //$('.mercury-qr-place-body').html(img);
                        //$('.mercury-preloader-close').detach();
                    },
                }).fail(function(xhr, status, error){
                    $('.mercury-qr-place').append(closeBtn);
                    $('.mercury-qr-place-body').html("Try again latter");
                }).then(function(result) {
                    createQr(result, crypto);

                    let delay = 3000;
                    let counter = 0;
                    let uuid = result.data['uuid'];

                    timerId = setTimeout(function request () {
                        clearTimeout(timerId);
                        $.ajax({
                            type: 'POST',
                            url: '<?php echo $this->getAjaxUrlCheckStatus() ?>',
                            data: {'uuid': uuid},
                            dataType: 'json',
                            success: function(data) {
                                status = data.data['status'];
                            }
                        });
                        counter += delay;

                        if(status === 'TRANSACTION_APROVED') {
                            successCallback();
                        } else if (status === 'TRANSACTION_RECEIVED') {
                            clearTimeout(stTimer);
                            $('.mercury-qr-adres-timer-name').addClass('.mercury-qr-adres-timer-name-agree').html("Payment received, please wait while it’s confirmed in the blockchain. Do not close this window until the payment is completed");
                            $('#timer').html();
                            timerId = setTimeout(request, delay);
                        } else {
                            if(counter < 60000 * 15) {
                                timerId = setTimeout(request, delay);
                            } else {
                                $('.mercury-qr-adres-timer-name').html("Transaction Expired, Try Again");
                                $('#timer').html('00' + " : " + '00');
                                $('.mercury-preloader-close').removeClass('mercury-preloader-close-alert');
                                clearTimeout(stTimer);
                                clearTimeut(timerId);
                            }
                        }

                    }, delay);
                });
            });

            function createQr(data, crypto) {
                let cryptoAmount = data.cryptoAmount,
                    fiatIsoCode = data.fiatIsoCode,
                    fiatAmount = data.fiatAmount,
                    rate = data.rate,
                    adres = data.address,
                    fee = data.fee;

                let names = {
                    'ETH' : 'ethereum',
                    'BTC' : 'bitcoin',
                    'DASH' : 'dash'
                };

                let header = '<div class="mercuryqpb-tit">Amount to be Paid<span>' + cryptoAmount + " " + crypto +'</span></div>',
                    list = '<ul class="mercuryqpb-list">';

                list += '<li><span>Amount to be paid ' + fiatIsoCode + ':</span><span>' + fiatAmount + " " + fiatIsoCode + '</span></li>';
                list += '<li><span>Exchange rate:</span><span>' + rate +  " " + fiatIsoCode + '</span></li>';
                list += '<li><span>Total:</span><span>' + cryptoAmount + " " + crypto + '</span></li>';

                let suggest = '<ul class="mercuryqpb-list">' +
                    '<li><span>Suggested network coast</span><span>' + fee + " " + crypto + '</span></li>' +
                    '</ul>';

                let qrStr = "";
                qrStr += names[crypto] + ":" + adres + "?";
                qrStr += "amount=" + cryptoAmount + "&";
                qrStr += "cryptoCurrency=" + crypto;

                let img = '<div class="mercury-qr-img"></div>';

                let adres_button = '<span class="mercury-qr-adres-button">Show Wallet Address</span>';

                let closeBtn = '<div class="mercury-preloader-close mercury-preloader-close-alert">✕</div>';
                let timer = '<div class="mercury-qr-adres-timer"><span class="mercury-qr-adres-timer-name">The transaction will expire in:</span><span id="timer"></span></div>';

                $('.mercury-qr-place-body').html(header);
                $('.mercury-qr-place-body').append(closeBtn);
                $('.mercury-qr-place-body').append(list);
                $('.mercury-qr-place-body').append(img);
                $('.mercury-qr-place-body').append(adres_button);
                $('.mercury-qr-place-body').append(suggest);
                $('.mercury-qr-place-body').append(timer);

                $('#timer').html(017 + ":" + 00);

                startTimer();

                showQr(qrStr);

                $('.mercury-qr-adres-button').on('click', function(){
                    if($(this).hasClass('active')) {
                        $('.mercury-qr-img').html("");
                        showQr(qrStr);
                        $(this).removeClass('active');
                        $(this).text("Show Wallet Address");
                    } else {
                        $('.mercury-qr-img').html(adres);
                        $(this).addClass('active');
                        $(this).text("show the qr code");
                    }
                });
            }

